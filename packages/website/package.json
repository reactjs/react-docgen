{
  "name": "@react-docgen-internal/website",
  "private": true,
  "version": "0.0.0",
  "description": "react-docgen website",
  "scripts": {
    "dev": "nx exec -- next dev",
    "build": "NODE_ENV=production nx exec -- next build",
    "start": "nx exec -- next start"
  },
  "author": "Daniel Tschinder (http://github.com/danez)",
  "license": "MIT",
  "dependencies": {
    "@codemirror/lang-javascript": "6.2.2",
    "@codemirror/lang-json": "6.0.1",
    "@codemirror/view": "6.34.1",
    "@headlessui/react": "1.7.19",
    "@popperjs/core": "2.11.8",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@uiw/react-codemirror": "4.23.5",
    "clsx": "2.1.1",
    "next": "14.2.15",
    "next-themes": "0.3.0",
    "nextra": "2.13.4",
    "nextra-theme-docs": "2.13.4",
    "postcss": "8.4.47",
    "postcss-lightningcss": "1.0.1",
    "react": "18.3.1",
    "react-docgen": "workspace:7.1.0",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.14"
  },
  "browserslist": [
    "chrome 64",
    "edge 79",
    "firefox 67",
    "opera 51",
    "safari 12"
  ],
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{projectRoot}/.next"
        ]
      },
      "dev": {
        "dependsOn": [
          "^build"
        ]
      },
      "start": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
